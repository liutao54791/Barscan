void __fastcall TDialogBase::PanelButtonResize(TObject *Sender)
{
    int nHalf = PanelButton->Width / 2;
    cxButtonOk->Left = nHalf - cxButtonOk->Width - 10;
    cxButtonCancle->Left = nHalf + 10;
}
